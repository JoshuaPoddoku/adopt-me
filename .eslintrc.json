{
  "extends": [
    "eslint : recommended",
    "plugin: import/erros",
    "plugin : react/recommended",
    "plugin : jsx",
    "plugin : jsx-a11y/recommended", //accessibility
    "prettier",
    "prettier/react"
  ],
  "rules": {},
  "plugins": ["react", "import", "jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
